<div class="form form-category">
  <div class="form-header">
    <span>
      <%if @account_type == getAccountCardType.to_s(10)%>
        <%=t(:form_edit_account_card_title)%>
      <%elsif @account_type == getAccountCashType.to_s(10)%>
        <%=t(:form_edit_account_cash_title)%>
      <%end%>
    </span>
  </div>
  <div class="form-body">
    <%= render 'form' %>
    <%= link_to t(:button_show), @account, :class => "link" %>
    <%= link_to t(:button_back), accounts_path(:type => @account_type), :class => "link" %>
  </div>
</div>
