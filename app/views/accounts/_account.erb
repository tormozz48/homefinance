<tr>
  <td class="td-center">
    <%= link_to edit_account_path(account, :type => @account_type), :class => 'btn' do%>
        <i class="icon-edit"></i>
    <%end%>
    <%= link_to account, confirm: t('message.confirm.delete'), method: :delete, :class => 'btn' do%>
        <i class="icon-remove"></i>
    <%end%>
  </td>
  <td><%=account.amount%></td>
  <td><%=account.name%></td>
  <td><%=account.description%></td>
</tr>