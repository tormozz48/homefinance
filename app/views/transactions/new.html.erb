<div class="form form-transaction">
  <div class="form-header">
    <span>
      <%=getNewTitle(@transaction.transaction_type)%>
    </span>
  </div>
  <div class="form-body">
    <%= render 'form' %>
    <span class="tooltip">
        <span>
          <%=t('tooltip.back')%>
        </span>
        <%= link_to t('button.back'), transactions_path(:type => @transaction.transaction_type, :page => session[:page]), :class=>"link" %>
    </span>
  </div>
  <div class="form-footer"></div>
</div>
